<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="com.journaldev.hibernate.data.Nodos" table="dbo.Nodos">
		<id name="id" column="Id" type="long">
			<generator class="native" />
		</id>
		<property name="tipo" column="Tipo" type="int"/>
		<property name="nombre" column="Nombre" type="string"/>
		<property name="posX" column="PosX" type="int"/>
		<property name="posY" column="PosY" type="int"/>
		<property name="label" column="Label" type="string"/>
	</class>
</hibernate-mapping>