<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="com.journaldev.hibernate.data.TablaMaestra" table="TablaMaestra">
		<id name="id" column="id" type="long">
			<generator class="native" />
		</id>
		<property name="version" column="version" type="integer"/>
		<property name="fechaCreacion" column="fecha_creacion" type="date"/>
		<property name="estaVigente" column="esta_vigente" type="boolean"/>
	</class>	
</hibernate-mapping>